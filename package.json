{
  "name": "moogy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --env=node --colors --coverage test"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.5",
    "eslint": "^6.7.2",
    "jest": "^27.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
